{
  "name": "Find Font",
  "version": "1.0.0",
  "description": "Find any font from a website",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css": ["style.css"],
      "js": ["main.js"],
      "match_about_blank": false
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3,
  "action": {},
  "permissions": [
    "activeTab"
  ]
}
